<MyClaims>:
    name:"myClaims"
    BoxLayout:
        orientation: 'vertical'
        MDTopAppBar:
            title:"My Claims"
            elevation: 0
            pos_hint: {"top": 1}
            md_bg_color:"#323B4E"
            left_action_items: [["arrow-left", lambda x : root.back(self.icon)]]
        BoxLayout:
            orientation: 'vertical'
            md_bg_color:"#caf0f8"
            ScrollView:
                size_hint: (1,1)
                MDList:
                    id: claims

        