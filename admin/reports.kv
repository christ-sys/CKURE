<Reports>:
    name: "reports"
    FloatLayout:
        MDRaisedButton:
            text: "Print Reports Data"
            pos_hint: {'center_x': .5, 'center_y': .10}
            # on_release : root.print_data()
            elevation: 0
    MDNavigationLayout:
        ScreenManager:
            id: screen_manager
            
    MDTopAppBar:
        title: "CKURE - ALL REPORTS"
        elevation: 0
        pos_hint: {"top": 1}
        md_bg_color:(0/255, 139/255, 199/255)
        left_action_items:
            [['menu', lambda x: nav_drawer.set_state("open")]]
    MDNavigationDrawer:
        id: nav_drawer
        radius: (0, 16, 16, 0)
        md_bg_color:"#abdbe3"
        ContentNavigationDrawer:
            sm: screen_manager
            nav_drawer: nav_drawer


      
