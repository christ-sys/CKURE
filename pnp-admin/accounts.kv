<Accounts>:
    name: "accounts"
    MDScreen:
        MDTopAppBar:
            title:"Accounts"
            elevation: 0
            pos_hint: {"top": 1}
            md_bg_color:"#323B4E"
            left_action_items:
                [['menu', lambda x: nav_drawer.set_state("open")]]
            right_action_items: [["account", lambda x: x]]
    ScrollView:
        pos_hint: {"center_x": 0.5, "center_y": 0.38}
        size_hint: (1,1)
        GridLayout:
            id: grid
            cols: 1
            height: self.minimum_height
            size_hint_y: None
            padding: "10dp", "10dp", "10dp", "50dp"
            MDList:
                id: user_list
            
    AnchorLayout:
        anchor_x: "center"
        anchor_y: 'bottom'
        padding: ('30dp', '30dp', '30dp', '30dp')
        MDRaisedButton:
            text: 'Print Record'
            md_bg_color:"#323B4E"
            size_hint_x: 1
    MDNavigationLayout:
        ScreenManager:
            id: screen_manager
        MDNavigationDrawer:
            id: nav_drawer
            radius: (0, 16, 16, 0)
            ContentNavigationDrawer:
                sm: screen_manager
                nav_drawer: nav_drawer