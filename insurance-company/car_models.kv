<Model>:
    name: "car_models"
    MDTopAppBar:
        title: "Models"
        elevation: 0
        pos_hint: {"top": 1}
        md_bg_color:"#0096c7"
        left_action_items:[["arrow-left", lambda x : root.back(self.icon)]]
        right_action_items: [["car-search"]]
    ScrollView:
        pos_hint: {"center_x": 0.5, "center_y": 0.38}
        size_hint: (1,1)
        GridLayout:
            id: grid
            cols: 1
            height: self.minimum_height
            size_hint_y: None
            width: self.minimum_width
    AnchorLayout:
        padding: '10dp'
        anchor_y: 'bottom'
        anchor_x: "right"
        MDIconButton:
            icon: "plus"
            halign: "right"
            rounded: True
            md_bg_color:"#90e0ef"
            # on_press: root.add_brand()